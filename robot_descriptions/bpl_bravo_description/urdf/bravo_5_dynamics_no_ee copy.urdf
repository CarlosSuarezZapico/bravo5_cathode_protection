<!-- |    Modified: 26-Nov-2025                                                        | -->
<!-- |    This version defines the continuous joints (1 and 4) as continuous           | -->
<!-- =================================================================================== -->
<?xml version="1.0"?>
    <robot name="bravo5">
        <material name="green">
            <color rgba="0.235 0.702 0.443 1"/>
        </material>
        <material name="blue">
            <color rgba="0.263 0.733 0.984 1"/>
        </material>
        <material name="red">
            <color rgba="1 0 0 1"/>
        </material>
        <material name="black">
            <color rgba="0.8 0.8 0.8 1"/>
        </material>
        <material name="orange">
            <color rgba="1 0.369 0.047 1"/>
        </material>

        <link name="link1">
            <visual>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/RS2-266.stl"/>
                </geometry>
                <material name="blue"/>
            </visual>
            <inertial>
                <origin xyz="0.0525 -0.001 0.04345" rpy="0 0 0"/>
                <mass value="1.85"/>
                <inertia ixx="0.002528" ixy="-2.0e-05" ixz="2.3e-05" iyy="0.002593" iyz="-7.0e-06" izz="0.003312"/>
            </inertial>
            <collision>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/RS2-266_convex_hull.stl"/>
                </geometry>
            </collision>
        </link>

        <joint name="joint1" type="continuous">
            <parent link="link1"/>
            <child link="link2"/>
            <origin rpy="3.141592654 0 0" xyz="0.0665 0 0.078"/>
            <axis xyz="0 0 1"/>
        </joint>

        <link name="link2">
            <visual>
                <origin xyz="0 0 0" rpy="-3.141592654 0 0"/>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/RS2-217.stl"/>
                </geometry>
                <material name="green"/>
            </visual>
            <inertial>
                <origin xyz="-0.03 -0.01 -0.06145" rpy="0 0 0"/>
                <mass value="1.62"/>
                <inertia ixx="0.003179" ixy="-0.000233" ixz="-0.001024"
                        iyy="0.004209" iyz="-0.000344" izz="0.002387"/>
            </inertial>
            <collision>
                <origin xyz="0 0 0" rpy="-3.141592654 0 0"/>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/RS2-217_convex_hull.stl"/>
                </geometry>
            </collision>
        </link>

        <joint name="joint2" type="revolute">
            <parent link="link2"/>
            <child link="link3"/>
            <origin rpy="3.141592654 0 0" xyz="-0.046 0 -0.0674"/>
            <axis xyz="0 1 0"/>
            <limit effort="9.0" lower="0.0" upper="3.5" velocity="0.5"/>
        </joint>

        <link name="link3">
            <visual>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/RS2-250-212-214.stl"/>
                </geometry>
                <material name="blue"/>
            </visual>
            <inertial>
                <origin xyz="-0.0241436 0.00399889 -0.1620485" rpy="0 0 0"/>
                <mass value="2.12"/>
                <inertia ixx="0.0352226436" ixy="-2.26528218e-05" ixz="0.00129923425" iyy="0.0359810399" iyz="0.00315070263" izz="0.00336331658"/>
            </inertial>
            <collision>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/RS2-250-212-214_convex_hull.stl"/>
                </geometry>
            </collision>
        </link>

        <joint name="joint3" type="revolute">
            <parent link="link3"/>
            <child link="link4"/>
            <origin rpy="0 0 0" xyz="-0.0052 0 -0.29055"/>
            <axis xyz="0 1 0"/>
            <limit effort="9.0" lower="0.0" upper="3.5" velocity="0.5"/>
        </joint>

        <link name="link4">
            <visual>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/RS2-268.stl"/>
                </geometry>
                <material name="green"/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/RS2-268_convex_hull.stl"/>
                </geometry>
                <material name="green"/>
            </collision>
            <inertial>
                <origin xyz="0.03499989 -0.00300155 0.05931046" rpy="0 0 0"/>
                <mass value="1.33"/>
                <inertia ixx="0.001809" ixy="-0.000156" ixz="-0.000398" iyy="0.002024" iyz="-0.000197" izz="0.001557"/>
            </inertial>
        </link>

        <joint name="joint4" type="continuous">
            <parent link="link4"/>
            <child link="link5"/>
            <origin rpy="0 0 0" xyz="0.0408 0 0.09695"/>
            <axis xyz="0 0 1"/>
        </joint>

        <link name="link5">
            <visual>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/RS2-180.stl"/>
                </geometry>
                <material name="blue"/>
            </visual>
            <inertial>
                <origin xyz="-0.000999865 -0.00000344664 0.0247494355" rpy="0 0 0"/>
                <mass value="1.09"/>
                <inertia ixx="0.00197699995" ixy="1.88170062e-10" ixz="-1.50015577e-05" iyy="0.00198600003" iyz="-9.87966436e-07" izz="0.00103500002"/>
            </inertial>
            <collision>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/RS2-180_convex_hull.stl"/>
                </geometry>
            </collision>
        </link>

        <link name="end_effector_ball">
            <visual>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/end_effectors/contact_sphere.stl"/>
                </geometry>
                <material name="red"/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="package://bravo_ros_server/meshes/end_effectors/contact_sphere_convex_hull.stl"/>
                </geometry>
                <material name="red"/>
            </collision>
        </link>

        <joint name="bravo_EE_joint" type="fixed">
            <origin rpy="0 -1.57 0" xyz=" 0.045 -0.035 0.113"/>
            <parent link="link5"/>
            <child link="end_effector_ball"/>
        </joint>

        <link name="contact_point"/>
        <joint name="contact_point_joint" type="fixed">
            <origin rpy="0 0 0" xyz="0 0 0.24"/>
            <parent link="link5"/>
            <child link="contact_point"/>
        </joint>
    </robot>
